{
  "id": "jobs",
  "label": "Trabalhos",
  "format": "custom",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "uid": { "config": { "label": "UID" }, "type": "UID" },
      "jobTitle": {
        "type": "Text",
        "config": { "label": "Titulo do Trabalhos", "placeholder": "" }
      },
      "jobNames": {
        "type": "Text",
        "config": { "label": "Nomes", "placeholder": "" }
      },
      "mainImageJob": {
        "type": "Image",
        "config": {
          "label": "Imagem Principal",
          "constraint": {},
          "thumbnails": []
        }
      },
      "jobLocale": {
        "type": "Text",
        "config": { "label": "Localização", "placeholder": "" }
      },
      "jobDate": {
        "type": "Date",
        "config": { "label": "Data do Trabalho", "placeholder": "" }
      },
      "category": {
        "type": "Link",
        "config": {
          "label": "Categoria",
          "select": "document",
          "customtypes": ["categories", "jobs"]
        }
      },
      "description": {
        "type": "StructuredText",
        "config": {
          "label": "Descrição",
          "placeholder": "",
          "allowTargetBlank": true,
          "multi": "paragraph,preformatted,heading1,heading2,heading3,heading4,heading5,heading6,strong,em,hyperlink,image,embed,list-item,o-list-item,rtl"
        }
      },
      "slices": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": { "choices": { "gallery": { "type": "SharedSlice" } } }
      }
    }
  }
}
